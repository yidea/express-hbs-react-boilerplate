{
  "name": "express-hbs-boilerplate",
  "version": "0.0.1",
  "description": "express-hbs-boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- --stage 1 --optional strict --watch server --ext js,hbs server/index.js",
    "server": "nodemon --exec babel-node -- --stage 1 --optional strict --watch client --watch server --ext js,jsx server/index.js",
    "server-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "prod": "babel-node --stage 1 -- server/index.js",
    "hot": "concurrent -kr 'npm run server-hot' 'npm run server'",
    "build": "webpack"
  },
  "author": "Yi Cao",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-stylus": "^0.7.1",
    "babel": "^5.6.1",
    "babel-core": "^5.6.1",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.6.1",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.13.3",
    "clean-webpack-plugin": "^0.1.3",
    "compression": "^1.5.2",
    "concurrently": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.3.5",
    "exphbs": "^1.0.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "json-loader": "^0.5.2",
    "lodash": "^3.10.1",
    "method-override": "^2.3.5",
    "mime-types": "^2.1.5",
    "mongoose": "^4.1.5",
    "morgan": "^1.6.1",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^1.2.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.2",
    "request": "^2.55.0",
    "serve-favicon": "^2.3.0",
    "songbird": "^1.0.1",
    "webpack": "^1.12.1",
    "webpack-stats-plugin": "0.1.0",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^2.0.2",
    "eslint": "^1.2.1",
    "eslint-config-defaults": "^4.2.0",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.2.3",
    "react-hot-loader": "^1.2.8",
    "nodemon": "^1.4.0",
    "webpack-dev-server": "^1.10.1"
  }
}
